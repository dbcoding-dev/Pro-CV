     function logout() {
      fetch('/panel/logout', {
        method: 'POST', 
      })
      .then(response => {
        if (response.redirected) {
          window.location.href = '/panel/login';
        } else {
          console.log('Logout successful');
        }
      })
      .catch(error => {
        console.error('Logout error:', error);
      });
    }
